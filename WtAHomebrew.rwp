function onFirstLayout()
	if string.len(DB.getValue(window.getDatabaseNode(),"str_action","")) &gt; 0 then
		lock(true);
		setReadOnly(false);
	else
		lock(false);
		setReadOnly(true);
	end
end

function onClickRelease(button, x, y)
	if isLocked() then
		window.str_action.setValue("");
		window.dt_action.setValue("");
		clearAllNodes();
	end	
end

function clearAllNodes() 
	local node = window.getDatabaseNode();
		
	for sClass, sRecord in pairs(node.getChildren()) do
		
		if not(sClass == "name" or sClass == "link" or sClass == "lock_action" or sClass == "dt_routine" or sClass == getName()) then
			--Destroy node
			DB.deleteChild(node, sClass);
		end
	end
	window.lbl_reset.setVisible(true);
end</LUACode>
