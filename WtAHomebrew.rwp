    <LUACode>
function onClickRelease(button, x, y)
	--get current Date
	local nSerialCurrentDate = MoonManager.getSerialCurrentDate();
	local nodeChar = window.getDatabaseNode();
	
	-- check if used today
	if nSerialCurrentDate == 0 then
		local msg = 
		{
			font = "whisperfont", 
			mode = "whisper",
			icon = "icon_moon_waxing_crescent";
			text = "Refresh the moon first";
		}
		Comm.addChatMessage(msg);
	elseif nSerialCurrentDate == window.num_serial_rest.getValue() then
		local msg = 
		{
			font = "whisperfont", 
			mode = "whisper",
			icon = "portrait_" .. nodeChar.getName() .. "_chat";
			text = nodeChar.getChild("name").getValue().." has already gained Willpower from Resting today";
		}
		Comm.addChatMessage(msg);
	else
		--Apply Willpower Rest
		window.num_serial_rest.setValue(nSerialCurrentDate);
		local nValue = window.num_willpower.getValue();
		nValue = nValue + 1;
		window.num_willpower.setValue(nValue);
	end
end
</LUACode>
