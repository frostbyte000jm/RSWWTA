function onClickRelease(button, x, y)
	local nSerialCurrentDate = MoonManager.getSerialCurrentDate();
	--local nodeChar = window.getDatabaseNode();
	local node = DB.findNode("partysheet.partyinformation");
    --Debug.chat("node:",node);
    
    --Single node Traversal
    for sClass,rRecord in pairs(node.getChildren()) do
        --Debug.chat("sClass: ",sClass," rRecord: ",rRecord);
        local sNodeClass, sNodeChar = rRecord.getChild("link").getValue();
        --Debug.chat("sNodeClass",sNodeClass,"sNodeChar",sNodeChar);
        local nodeChar = ActorManager.getCreatureNode(sNodeChar);
        --Debug.chat("nodeChar",nodeChar);
        
        if nSerialCurrentDate == nodeChar.getChild("num_serial_moon").getValue() then
			local msg = 
			{
				font = "whisperfont", 
				mode = "whisper",
				icon = "portrait_" .. nodeChar.getName() .. "_chat";
				text = nodeChar.getChild("name").getValue().." has already gained Rage from the Moon tonight";
			}
			Comm.addChatMessage(msg);
		else
			--Apply Rage
			nodeChar.getChild("num_serial_moon").setValue(nSerialCurrentDate);	
			--Debug.chat("nodeChar:",nodeChar);	
			local sAuspice = nodeChar.getChild("str_auspice").getValue();
			
			local nMoonIndex = MoonStack.getMoonIndex();
			local nMoonRage = MoonStack.getRageAmount();
			local bFullRage = false;
			
			if sAuspice then
				--Debug.chat("Rage Auspice:",sAuspice);
				-- check if moon on Auspice
				if sAuspice == "Ragabash" and nMoonIndex == 0 then
					bFullRage = true;
				elseif sAuspice == "Theurge" and (nMoonIndex == 1 or nMoonIndex == 7) then
					bFullRage = true;
				elseif sAuspice == "Philodox" and (nMoonIndex == 2 or nMoonIndex == 6) then
					bFullRage = true;
				elseif sAuspice == "Galliard" and (nMoonIndex == 3 or nMoonIndex == 5) then
					bFullRage = true;
				elseif sAuspice == "Ahroun" and nMoonIndex == 4 then
					bFullRage = true;
				end
			end
			
			-- Create Message
			local msg = 
			{
				font = "whisperfont", 
				mode = "ooc",
				icon = "portrait_" .. nodeChar.getName() .. "_chat";				
			}
			
			-- Apply Rage
			if bFullRage then
				nodeChar.getChild("num_rage").setValue(nodeChar.getChild("num_rage_max").getValue());
				msg.text = nodeChar.getChild("name").getValue().." gains all of their Rage, for seeing their Auspice moon.";
			else
				local nValue = nodeChar.getChild("num_rage").getValue();
				nValue = nValue + nMoonRage;
				nodeChar.getChild("num_rage").setValue(nValue); 
				msg.text = nodeChar.getChild("name").getValue().." gains "..nMoonRage.." Rage, for seeing the moon.";
			end
			
			-- Display message
			
			Comm.deliverChatMessage(msg);
		end
	end
end</LUACode>
