function createHandler()
	local wLink = window.link_effect;
	local sClass, _ = wLink.getValue();
	
	if string.len(sClass) &gt; 0 then
		local node = wLink.getTargetDatabaseNode();
		local path = DB.getPath(node,"name");
		DB.addHandler(path, "onUpdate", update);
		update(DB.getChild(node, "name"));
	end	
end

function update(node)
	setValue(DB.getValue(node, "", ""));
end
