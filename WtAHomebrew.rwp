	local bShow = false;
	if sTrigger == FBEffectsManager.StrEffectList.rTriggers.Activate[2] then
		bShow = true;
	end
	
	window.lbl_uses.setVisible(bShow);
	window.num_uses.setVisible(bShow);	
	
	if bShow == false then
		setValue(0);
	end
end</LUACode>
