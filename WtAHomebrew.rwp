    <LUACode>function onInit()
    calcTotals();
end

function onListRearranged(listchanged)
    calcTotals();
end

function calcTotals()
    --declarations
    local nXPSpent = 0;    
    local winXP = getWindows(); -- Traveling up from a windowlist.
    --Debug.chat("winXP: ",winXP);
    
    for sWinClass,rWinRecord in pairs(winXP) do
        --Debug.chat("sWinClass: ",sWinClass," rWinRecord: ",rWinRecord);
        local node = rWinRecord.getDatabaseNode();
        local nXP = node.getChild("num_xp").getValue();
        --Debug.chat("nXP:",nXP);
        
        --add to XP Spent
        nXPSpent = nXPSpent + nXP;          
    end
    
    --set XP spent
    window.num_xp_spent.setValue(nXPSpent);    
end


