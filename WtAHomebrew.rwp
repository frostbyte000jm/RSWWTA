function onClickRelease(button, x, y)
	--get current Date
	local nSerialCurrentDate = MoonManager.getSerialCurrentDate();
	local sCurrentDate = MoonManager.getStringCurrentDate();
	local nodeChar = window.getDatabaseNode();
	
	-- check if used today
	if nSerialCurrentDate == 0 then
		local msg = 
		{
			font = "whisperfont", 
			mode = "whisper",
			icon = "icon_moon_waxing_crescent";
			text = "Refresh the moon first";
		}
		Comm.addChatMessage(msg);
	elseif nSerialCurrentDate == window.num_serial_meditate.getValue() then
		local msg = 
		{
			font = "whisperfont", 
			mode = "whisper",
			icon = "portrait_" .. nodeChar.getName() .. "_chat";
			text = nodeChar.getChild("name").getValue().." has already meditated today.";
		}
		Comm.addChatMessage(msg);
	else
		--Send to Roller
		local rAddInfo = {
			sReturnLoc = "garou_meditate",			
		};
		local rStats = {manager_char_stat_handler.StatList.Attributes.Mental.Wits[2],manager_char_stat_handler.StatList.Skills.Knowledges.Enigmas[2]};
		manager_char_stats.AddStatsToRoll(nodeChar, rStats);
		FBRollManager.rollInit(nodeChar,"Action",rAddInfo);
		
		--Apply Meditation Date
		window.num_serial_meditate.setValue(nSerialCurrentDate);
		window.str_date_meditate.setValue(sCurrentDate);
		local nValue = window.num_meditate_mod.getValue();
		nValue = math.min(nValue + 1,10);
		window.num_meditate_mod.setValue(nValue);
	end
end

function onHover(state)
	window.num_meditate_mod.setDiff();
	
	setTooltipText("Last Meditated on : "..window.str_date_meditate.getValue()..". Today is: "..MoonManager.getStringCurrentDate());
end
