function onInit()
	if Session.IsHost then
		isGMPC();
	end
end

function isGMPC()
	local node = window.getDatabaseNode();
	local owner = node.getOwner();
		
	if owner == "GM" then
		setValue(1);
		return true;
	else
		setValue(0);
		return false;
	end
end</LUACode>
