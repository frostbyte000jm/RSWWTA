function toggleShow(sTrigger)
	local bShow = false;
	if sTrigger == FBEffectsManager.StrEffectList.rTriggers.BoT[2] 
	or sTrigger == FBEffectsManager.StrEffectList.rTriggers.EoT[2] 
	or sTrigger == FBEffectsManager.StrEffectList.rTriggers.RollType[2] 
	or sTrigger == FBEffectsManager.StrEffectList.rTriggers.DmgType[2] 
	or sTrigger == FBEffectsManager.StrEffectList.rTriggers.Resource[2] 
	or sTrigger == FBEffectsManager.StrEffectList.rTriggers.Activate[2] 
	or sTrigger == FBEffectsManager.StrEffectList.rTriggers.Static[2] 
	then
		bShow = true;
	end
	
	window.duration.setVisible(bShow);
	window.lbl_duration.setVisible(bShow);	
	window.box_tick_eot.setVisible(bShow);
	window.lbl_tick_eot.setVisible(bShow);
	window.box_tick_eos.setVisible(bShow);
	window.lbl_tick_eos.setVisible(bShow);
	window.isgmonly.setVisible(bShow);
	window.lbl_isgmonly.setVisible(bShow);
	window.box_start_inactive.setVisible(bShow);
	window.lbl_start_inactive.setVisible(bShow);
	
	if bShow == false then
		setValue(0);
	end
end</LUACode>
