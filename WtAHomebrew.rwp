function updateValue()
	local nAggro = window.num_health_aggro.getValue();
	local nLethal = window.num_health_lethal.getValue();
	local nBashing = window.num_health_bashing.getValue();
	local nCurrent = math.max(nBashing, math.max(nLethal, nAggro));
	local nTotal = window.num_health_total.getValue();
	
	if nTotal ~= 0 then
		if nAggro == nTotal then
			setValue(1);
		else
			setValue(nCurrent/nTotal);
		end
		
	end
end</LUACode>
