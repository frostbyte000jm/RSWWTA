    update();
end

function onValueChanged()
    update();
end

function update()
    local count = getValue();
    
    window.btn_health_01.setVisible(count&gt;=1);
    window.btn_health_02.setVisible(count&gt;=2);
    window.btn_health_03.setVisible(count&gt;=3);
    window.btn_health_04.setVisible(count&gt;=4);
    window.btn_health_05.setVisible(count&gt;=5);
    window.btn_health_06.setVisible(count&gt;=6);
    window.btn_health_07.setVisible(count&gt;=7);
    window.btn_health_08.setVisible(count&gt;=8);
    window.btn_health_09.setVisible(count&gt;=9);
    window.btn_health_10.setVisible(count&gt;=10);
    window.btn_health_11.setVisible(count&gt;=11);
    window.btn_health_12.setVisible(count&gt;=12);
    window.btn_health_13.setVisible(count&gt;=13);
    window.btn_health_14.setVisible(count&gt;=14);
    window.btn_health_15.setVisible(count&gt;=15);
    
    --local node = window.getDatabaseNode();
    --HealthManager.displayDamage(node);
    updateDamage();
end

function updateDamage()
    local node = window.getDatabaseNode();
    
    --declarations
    local nBashing = node.getChild("num_health_bashing").getValue();
    local nLethal = node.getChild("num_health_lethal").getValue();
    local nAggro = node.getChild("num_health_aggro").getValue();
    local nHealthMax = node.getChild("num_health_total").getValue();
    local nHealthStart = 15
        
    -- aggro first, then lethal, and then Bashing
    for i=nHealthStart,1,-1 do
        if i&gt;nHealthMax then
            node.getChild("btn_health_"..string.format("%02d",i)).setValue(0);
        elseif nAggro &gt; 0 then
            nAggro = nAggro - 1;
            node.getChild("btn_health_"..string.format("%02d",i)).setValue(3);
        elseif nLethal &gt; 0 then
            nLethal = nLethal - 1;
            node.getChild("btn_health_"..string.format("%02d",i)).setValue(2);
        elseif nBashing &gt; 0 then
            nBashing = nBashing - 1;
            node.getChild("btn_health_"..string.format("%02d",i)).setValue(1);
        else
            node.getChild("btn_health_"..string.format("%02d",i)).setValue(0);
        end
    end
    
    window.num_damage_pcnt.updateValue();
end</LUACode>
