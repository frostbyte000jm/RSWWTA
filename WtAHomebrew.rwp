function onInit()
    if self.getWindowCount()&gt;0 then
        return;
    end
    
    local node = DB.findNode("partysheet.partyinformation");
    --Debug.chat("node:",node);
    
    --Single node Traversal
    for sClass,rRecord in pairs(node.getChildren()) do
        --Debug.chat("sClass: ",sClass," rRecord: ",rRecord);
        local w = self.createWindow()
        local wNode = w.getDatabaseNode();
        local _,sCharNode = rRecord.getChild("link").getValue();
        --Debug.chat("sLinkV:",sLinkV,"sCharNode:",sCharNode);
        
        wNode.getChild("name").setValue(rRecord.getChild("name").getValue());
        wNode.getChild("str_char_node").setValue(sCharNode);
    end
end

