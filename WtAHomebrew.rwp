--[[
	Forms:
	1 - homid
	2 - glabro
	3 - crinos
	4 - hispo
	5 - lupus
]]--
function onInit()
	sendShiftButtonUpdate();
end

function onValueChanged()
	sendShiftButtonUpdate();
end

function sendShiftButtonUpdate()
	rControls = getControls();
	manager_garou.updateShiftButtons(window.getDatabaseNode(),rControls);
end

function getControls()
	local rControls = {
		btnHomid = window.btn_shift_homid,
		btnGlabro = window.btn_shift_glabro,
		btnCrinos = window.btn_shift_crinos,
		btnHispo = window.btn_shift_hispo,
		btnLupus = window.btn_shift_lupus,
	};
	
	return rControls;
end</LUACode>
