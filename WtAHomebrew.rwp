local doStress = false;

function onInit()
	DB.addHandler("combattracker.round", "onUpdate", updateButton);
	updateButton(DB.getChild("combattracker", "round"));
end

function onClickRelease(button, x, y)
	
	local bStress = doStress;
    if Input.isControlPressed() then
		bStress = not bStress;
    end
    
    local rAddInfo = {
		bStress = bStress,
    }
	
	FBRollManager.rollInit(nil,"Quick",rAddInfo);
end

function updateButton(node)
	if DB.getValue(node,"", 0) &gt; 0 then
		doStress = true;
		setTooltipText(Interface.getString("char_roll_stress"));
		setText("Roll", "Rolling", "Stress");
	else
		doStress = false;
		setTooltipText(Interface.getString("char_roll_calm"));
		setText("Roll", "Rolling", "Calm");
	end
end</LUACode>
