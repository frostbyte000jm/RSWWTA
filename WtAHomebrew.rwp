    <LUACode>--declarations
local psNode = nil;
local charNode = nil;

function onInit()
    --Debug.chat("ps_xp_detail onInit()");
    first();
    update();
	DB.addHandler("partysheet.partyinformation", "onChildUpdate", update);
end

function first()
    --Debug.chat("ps_xp_detail first()");
	-- set psNode
	psNode = getDatabaseNode();
	
	-- set charNode
	local _, sRecord = DB.getValue(psNode, "link", "", "");
	charNode = DB.findNode(sRecord);
	
	--Link base objects
	PartyManager.linkRecordField(charNode, psNode, "name", "string");
	PartyManager.linkRecordField(charNode, psNode, "token", "token", "token");
	
	-- Link additional items from character sheet
	--PartyManager.linkRecordField(charNode, psNode, "num_party", "number");	
end

function update()
    --Debug.chat("ps_xp_detail update()");
	
	PartyManager.linkRecordField(charNode, psNode, "num_xp_total", "number");
	PartyManager.linkRecordField(charNode, psNode, "num_xp_spent", "number");
	PartyManager.linkRecordField(charNode, psNode, "num_glory", "number");
	PartyManager.linkRecordField(charNode, psNode, "num_honor", "number");
	PartyManager.linkRecordField(charNode, psNode, "num_wisdom", "number");
	PartyManager.linkRecordField(charNode, psNode, "num_story_points", "number");
	PartyManager.linkRecordField(charNode, psNode, "num_rage", "number");
	PartyManager.linkRecordField(charNode, psNode, "num_rage_max", "number");
	PartyManager.linkRecordField(charNode, psNode, "num_gnosis", "number");
	PartyManager.linkRecordField(charNode, psNode, "num_gnosis_max", "number");
	PartyManager.linkRecordField(charNode, psNode, "num_willpower", "number");
	PartyManager.linkRecordField(charNode, psNode, "num_willpower_max", "number");
	
end
function onDrop(x, y, dragdata)
    --declarations
    --Debug.chat("dragdata:",dragdata);
    local questNode = dragdata.getDatabaseNode();
    --Debug.chat("questNode:",questNode);
    
    --Check Award
    local bContinue = questNode.getChild("cb_xpgiven").getValue();
    
    if bContinue == 1 then
        local msg = 
        {
            font = "whisperfont", 
            mode = "whisper",
            icon = "portrait_gm_token",
            text = "Award has already been given.";
        }
        Comm.deliverChatMessage(msg);
        return
    end
    
    local nBeats,nXP,nGlory,nHonor,nWisdom = award_exp.awardChar(charNode,questNode);
    
    --set Name
    local qName = questNode.getChild("name").getValue();
    qName = qName.." ("..charNode.getChild("name").getValue()..")";
    questNode.getChild("name").setValue(qName);
    
    --set Awarded
    questNode.getChild("cb_xpgiven").setValue(1);
    
    
    --Display Message
    local msg = 
    {
        font = "narratorfont", 
        mode = "ooc",
        icon = "portrait_" .. charNode.getName() .. "_chat",
        text = charNode.getChild("name").getValue().." Has received:\nBeats: "..nBeats.."\nXP: "..nXP.."\nGlory: "..nGlory.."\nHonor: "..nHonor.."\nWisdom: "..nWisdom;
    }
    Comm.deliverChatMessage(msg);       

end
