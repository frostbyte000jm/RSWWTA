    <LUACode>function onInit()
    widget = addBitmapWidget(activeicon[1]);
    FBCTObjects.registerObject("btn_ct_setactive_client",self);
    onOpen();
end

function onOpen()
	--declarations
	local nRound = window.round.getValue();
	local nodeActiveCT = CombatManager.getActiveCT();	
	local sType, nodeChar = ActorManager.getTypeAndNode(nodeActiveCT);	
	local bOwner = false;
	
	-- If round &gt; 0 and the type of node is PC, then check for ownership. If Owner, show active token.
	if nRound &gt; 0 and sType == "pc" then
		bOwner = DB.isOwner(nodeChar);		
	end
	
	setVisible(bOwner);
end

function onDragStart(button, x, y, draginfo)
    draginfo.setType("combattrackeractivation");
    draginfo.setIcon(activeicon[1]);
    widget.setVisible(false);
    
    return true;
end

function onDragEnd(draginfo)
    widget.setVisible(true);
end</LUACode>
