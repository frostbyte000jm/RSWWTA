function onInit()
	local node = window.getDatabaseNode();
	local dateNode = node.getChild("current");
	local dateNodePath = DB.getPath(dateNode);
	DB.addHandler(dateNodePath, "onChildUpdate", getMoonValue);
	update();
end

function getMoonValue()
	local nMoonYear = window.num_moon_year.getValue();
	local nMoonMonth = window.num_moon_month.getValue();
	local nMoonDay = window.num_moon_day.getValue();
	
	local nIndex = MoonManager.currentMoon(nMoonYear,nMoonMonth,nMoonDay);

	window.num_moon_index.setValue(nIndex);
end

function onValueChanged()
	update();
end

function update()
	local nIndex = getValue();
	if nIndex == 0 then
		window.icon_moon_phase.setIcon("icon_moon_new", true);
		window.num_moon_diff.setValue(8);			
	elseif nIndex == 1 then
		window.icon_moon_phase.setIcon("icon_moon_waxing_crescent", true);
		window.num_moon_diff.setValue(7);	
	elseif nIndex == 2 then
		window.icon_moon_phase.setIcon("icon_moon_first_quarter", true);
		window.num_moon_diff.setValue(6);	
	elseif nIndex == 3 then
		window.icon_moon_phase.setIcon("icon_moon_waxing_gibbous", true);
		window.num_moon_diff.setValue(5);	
	elseif nIndex == 4 then
		window.icon_moon_phase.setIcon("icon_moon_full", true);
		window.num_moon_diff.setValue(4);	
	elseif nIndex == 5 then
		window.icon_moon_phase.setIcon("icon_moon_waning_gibbous", true);
		window.num_moon_diff.setValue(5);	
	elseif nIndex == 6 then
		window.icon_moon_phase.setIcon("icon_moon_last_quarter", true);
		window.num_moon_diff.setValue(6);	
	elseif nIndex == 7 then
		window.icon_moon_phase.setIcon("icon_moon_waning_crescent", true);
		window.num_moon_diff.setValue(7);
	end
end
</LUACode>
