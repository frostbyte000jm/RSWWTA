    <LUACode>
function onInit()
	if Session.IsHost then
		registerMenuItem(Interface.getString("ct_menu_itemdeletenonfriendly"), "delete", 5);
		registerMenuItem(Interface.getString("ct_menu_itemdeletefoe"), "delete", 3);
	end		
end

function onClickDown(button, x, y)
	return true;
end

function onClickRelease(button, x, y)
	if button == 1 then
		Interface.openRadialMenu();
		return true;
	end
end

function onMenuSelection(selection, subselection, subsubselection)
	if Session.IsHost then
		if selection == 5 then
			clearNPCs();
		elseif selection == 3 then
			clearNPCs(true);
		end
	end
end

function clearNPCs(bDeleteOnlyFoe)
	--functions to run through each character	
	function clearFightNotFriend(nodeCT)
		FBCTManager.endCombat(nodeCT, "notFriend");		
	end
	
	function clearFightFoe(nodeCT)
		FBCTManager.endCombat(nodeCT, "foe");		
	end
	
	-- choose routine.
	if bDeleteOnlyFoe then
		CombatManager.callForEachCombatant(clearFightFoe);		
	else
		CombatManager.callForEachCombatant(clearFightNotFriend);			
	end
	
	-- reset inititive.
	CombatManager.resetInit();
	window.round.setValue(0);
	window.btn_combat_start.toggleClearAndFight();
end</LUACode>
