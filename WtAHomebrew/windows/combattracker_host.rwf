function onClickDown(button, x, y)
	return true;
end

function onClickRelease(button, x, y)
	clearSceneEffects();	
end

function clearSceneEffects()
	function clearCharSceneEffects(nodeEffect)
		if DB.getValue(DB.getChild(nodeEffect, "effect_action"),"box_tick_eos",0) == 1 then
			FBHandleEffects.removeEffect(nodeEffect.getParent().getParent(),nodeEffect);
		end
	end
	CombatManager.callForEachCombatantEffect(clearCharSceneEffects);
end</LUACode>
