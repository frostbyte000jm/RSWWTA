function onInit()
	toggleClearAndFight();
end

function onClickRelease(button, x, y)
	function startFight(nodeCT)
		FBCTManager.startCombat(nodeCT);		
	end
	CombatManager.callForEachCombatant(startFight);
	
	window.round.setValue(1);
	
	toggleClearAndFight();
end

function toggleClearAndFight()
	if window.round.getValue() &gt; 0 then
		window.btn_combat_start.setEnabled(false);
	else
		window.btn_combat_start.setEnabled(true);
	end
end

</LUACode>
