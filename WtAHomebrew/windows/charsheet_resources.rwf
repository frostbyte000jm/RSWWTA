    setValue(0);
end

function onClickRelease(button, x, y)
	local value = getValue();
	local nWill = window.num_willpower_current.getValue();
	
	-- If Button is unpressed
	if value == 0 then		
		--If there is any Willpower move will value to bucket.
		if nWill &gt; 0 then
			window.num_willpower_current.setValue(nWill - 1);            
			nIndexAdded = ModifierStack.addSlot("Bonus Willpower Success", 1);
		end
	else
	-- If button is pressed		
		-- If a value is in the bucket, move it back to the sheet.
		if nIndexAdded &gt; 0 then
			window.num_willpower_current.setValue(nWill + 1);            
			ModifierStack.removeSlot(indexAdded);
		end		
	end
end</LUACode>
