function onInit()
	local pathTrigger1 = getDatabaseNode().getPath();
	DB.addHandler(pathTrigger1..".*.dt_trigger_type", "onUpdate", toggleAttackType);
	toggleShow(window.dt_effect_trigger.getValue());
end

function toggleAttackType(node)
	window.dt_trigger_attack_type.toggleShow();
end

function toggleShow(sTrigger)
	local bShow = false;
	if sTrigger == FBEffectsManager.StrEffectList.rTriggers.RollType[2]
	or sTrigger == FBEffectsManager.StrEffectList.rTriggers.DmgType[2]
	or sTrigger == FBEffectsManager.StrEffectList.rTriggers.Resource[2] then
		bShow = true;
	end
	
	window.lbl_trigger_type_1.setVisible(bShow);
	window.btn_opt_trigger_andor_1.setVisible(bShow);
	window.icon_trigger_type_1.setVisible(bShow);
	window.wl_trigger_type_1.setVisible(bShow);
	window.scrollbar_trigger_1.setVisible(bShow);
end</LUACode>
