function toggleShow()
	local bShow = false;
	
	--Check all details if any are Attack Type, then show.
	for sClass,rRecord in pairs(window.wl_trigger_type_1.getDatabaseNode().getChildren()) do
        if DB.getValue(rRecord,"dt_trigger_type","") == FBRollManager.rStrRollList.rRollTypes.Attack[2] then
			bShow = true;
			break;
        end
    end
	
	window.lbl_trigger_attack_type.setVisible(bShow);
	window.dt_trigger_attack_type.setComboBoxVisible(bShow);
	
	if bShow == false then
		setValue("");
	end
end
