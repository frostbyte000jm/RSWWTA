function toggleShow(sTrigger)
	local bShow = false;
	if sTrigger == FBEffectsManager.StrEffectList.rTriggers.RollType[2]
	or sTrigger == FBEffectsManager.StrEffectList.rTriggers.DmgType[2] then
		bShow = true;
	end
	
	window.lbl_trigger_type_2.setVisible(bShow);
	window.btn_opt_trigger_andor_2.setVisible(bShow);
	window.icon_trigger_type_2.setVisible(bShow);
	window.wl_trigger_type_2.setVisible(bShow);
	window.scrollbar_trigger_2.setVisible(bShow);
end

