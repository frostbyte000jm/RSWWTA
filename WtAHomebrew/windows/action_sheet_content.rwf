    <LUACode>local doFirstRun = false;

function onInit()
	-- create or grab new tree
	local nodeChild = DB.createChild(window.getDatabaseNode(), "effect");
	
	-- if first run, set true.
	if not(DB.getValue(nodeChild,"box_effect_only",nil)) then		
		doFirstRun = true;
	end
	
	-- create subwindow
	setValue("action_sheet_action",DB.getPath(nodeChild));
	
	--set effect only node to be true
	if doFirstRun then
		DB.setValue(nodeChild,"box_effect_only","number",1);
		doFirstRun = false;
	end
	
	--showWindow()
	showWindow();
end

function showWindow()
	local bValue = window.box_effect.getValue() == 1;
	window.lbl_effect.setVisible(bValue);
	window.sw_effect.setVisible(bValue);
end

function setEffectButton(nValue)
	window.box_show_effect_effect.setValue(nValue);
end
