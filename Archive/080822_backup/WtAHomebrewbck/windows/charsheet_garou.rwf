    <LUACode>local nIndex = 3;

function setCurrent(bValue)
	if bValue then
		setValue(2);
	elseif DB.getValue(window.getDatabaseNode(), "num_breed_index",0) == nIndex then
		setValue(1);
		setReadOnly(false);
		return;
	else
		setValue(0);
	end
	setReadOnly(true);
end

function setOpen(nCurrent, nResult)
	if (nCurrent &gt; nIndex and nCurrent - nResult &lt;= nIndex) 
	or (nCurrent &lt; nIndex and nCurrent + nResult &gt;= nIndex) then
		setValue(1);
		setReadOnly(false);
	end	
end

function onClickRelease(button, x, y)
	local rStatValues = {
		nStrMod = window.num_crinos_str.getValue(),
		nDexMod = window.num_crinos_dex.getValue(),
		nStaMod = window.num_crinos_sta.getValue(),
		nManMod = window.num_crinos_man.getValue(),	
		nFormIndex = nIndex,	
	}	
	local nodeChar = window.getDatabaseNode();
	manager_garou.updateCharStats(nodeChar, window.num_form_index.getControls(), rStatValues);
	
	--set Difficulty and Regen
	DB.setValue(nodeChar, "num_shift_diff", "number", DB.getValue(nodeChar, "num_crinos_diff", 6));
	DB.setValue(nodeChar, "num_regen", "number", DB.getValue(nodeChar, "box_crinos_regen", 0));
	DB.setValue(nodeChar, "str_form", "string", Interface.getString("char_form_crinos"));
end</LUACode>
