	-- set nodePS
	nodePS = getDatabaseNode();
	
	-- set nodeChar
	local _, sRecord = DB.getValue(nodePS, "link", "", "");
	nodeChar = DB.findNode(sRecord);
	
	-- Revise Beats on Character Sheet
	local nBeats = nodeChar.getChild("num_story_points").getValue();
	nBeats = nBeats + 1;
	nodeChar.getChild("num_story_points").setValue(nBeats);
	
end
