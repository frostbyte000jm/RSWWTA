function onClickRelease(button, x, y)
	-- set nodePS
	nodePS = getDatabaseNode();
	
	-- set nodeChar
	local _, sRecord = DB.getValue(nodePS, "link", "", "");
	nodeChar = DB.findNode(sRecord);
	
	-- Revise Beats on Character Sheet
	local nValue = nodeChar.getChild("num_story_points").getValue();
	nValue = nValue + 1;
	nodeChar.getChild("num_story_points").setValue(nValue);
	
end
