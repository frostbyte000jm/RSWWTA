    <LUACode>--declarations
local psNode = nil;
local charNode = nil;

function onInit()
    --Debug.chat("ps_xp_detail onInit()");
    loadCharacterInfo()
end

function loadCharacterInfo()
	-- set psNode
	psNode = getDatabaseNode();
	
	-- set charNode
	local _, sRecord = DB.getValue(psNode, "link", "", "");
	charNode = DB.findNode(sRecord);
	
	--Link base objects
	PartyManager.linkRecordField(charNode, psNode, "name", "string");
	PartyManager.linkRecordField(charNode, psNode, "token", "token", "token");
	
	-- Link additional items from character sheet
	PartyManager.linkRecordField(charNode, psNode, "str_breed", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_auspice", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_tribe", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_char_concept", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_char_nature_demeanor", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_character_demeanor", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_char_secret", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_backgrounds", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_merits", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_flaws", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_question1", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_char_question1", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_question2", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_char_question2", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_question3", "string");
	PartyManager.linkRecordField(charNode, psNode, "str_char_question3", "string");
	
end

function getNodeChar()
	return charNode;
end
</LUACode>
