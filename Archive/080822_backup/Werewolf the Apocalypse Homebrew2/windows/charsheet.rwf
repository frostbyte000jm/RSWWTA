function onClickRelease(button, x, y)
    --get nodeChar
    local nodeChar = window.getDatabaseNode();
    
    --get Current Value and set new Value
    local nBeat = window.num_story_points.getValue();
    window.num_story_points.setValue(nBeat - 1);
    
    -- display message
    FBChatManager.sendMessage(nodeChar, DB.getValue(nodeChar,"name","").." Spends a Beat!", "action", false);
end</LUACode>
