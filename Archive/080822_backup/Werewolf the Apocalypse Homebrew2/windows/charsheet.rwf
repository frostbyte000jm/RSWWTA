    <LUACode>function onDoubleClick(x, y)
    --get charNode
    local charNode = window.getDatabaseNode();
    
    -- get Diff
    local nDiff = charNode.getChild("num_prior_difficulty").getValue();
    
    --get Current Value and set new Value
    local nBeat = window.num_story_points.getValue();
    window.num_story_points.setValue(nBeat - 1);
    
    -- display message
    local msg = 
    {
        font = "narratorfont", 
        mode = "ooc",
        icon = "portrait_" .. charNode.getName() .. "_chat";
        text = charNode.getChild("name").getValue().." Attempts a Chance Dice using a Beat!";
    }
    Comm.addChatMessage(msg);
    
    --Send to Roller
    FBRollManager.rollInit(charNode,"Beat",nDiff);
end


</LUACode>
