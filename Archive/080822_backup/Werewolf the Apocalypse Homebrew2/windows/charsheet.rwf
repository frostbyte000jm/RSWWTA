function onInit()
	DB.addHandler("combattracker.round", "onUpdate", updateBox);
	updateBox(DB.getChild("combattracker", "round"));
end

function updateBox(node)
	if DB.getValue(node,"", 0) &gt; 0 then
		setValue(1);
	else
		setValue(0);
	end
end</LUACode>
