function onInit()
	--updateLink();
	registerMenuItem(Interface.getString("create_effect"), "insert", 5);
	registerMenuItem(Interface.getString("remove_effect"), "delete", 3);
	registerMenuItem(Interface.getString("list_menu_deleteconfirm"), "delete", 3, 4);
	onFirstOpen();
end

function onFirstOpen()
	if getTargetDatabaseNode() == window.getDatabaseNode() then
		setValue("");
	end
end

function onMenuSelection(selection, subselection1, subselection2, subselection3)
	if selection == 5 then
		CreateEffect();
	elseif selection == 3 and subselection1 ==4 then
		RemoveEffect();
	end
end

function onDrop(x, y, dragdata)
	--declarations	
	local nodeDragdata = dragdata.getDatabaseNode();
	
	-- Check if effect_custom or effects. If so, copy the link, and add copy.
	if StringManager.startsWith(nodeDragdata.getNodeName(), "effect_custom.") or StringManager.startsWith(nodeDragdata.getNodeName(), "effects.") then
		CreateEffect(dragdata);
	end
end

function CreateEffect(dragdata)
	local nodeAction = window.getDatabaseNode();	
	local childEffect = DB.createChild(nodeAction, "effect_sheet");
	
	if dragdata then
		local nodeDragdata = dragdata.getDatabaseNode();
		DB.copyNode(nodeDragdata, childEffect);
	end
	
	setValue("effect_sheet",childEffect.getPath());
	local winEffect = Interface.openWindow("effect_sheet", childEffect.getPath());
	local nodeEffect = winEffect.getDatabaseNode();
	local pathAction = window.getDatabaseNode().getPath();
	
	--Set Add Effect Box
	if window.box_attack_add_effect.getValue() == 0 and window.box_attack_add_effect_success.getValue() == 0 then
		window.box_attack_add_effect.setValue(1);
	end	
	
	--create Handler
	window.str_effect_name.createHandler();
end

function RemoveEffect()
	local node = getTargetDatabaseNode();
	DB.deleteChildren(node);
	setValue("");
	window.str_effect_name.setValue("");
	window.box_attack_add_effect.setValue(0);
end
