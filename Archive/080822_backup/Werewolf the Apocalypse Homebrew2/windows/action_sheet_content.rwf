function onInit()
	if string.len(getValue()) == 0 then
		setValue(manager_char_stat_handler.StatList.rStatTypesFull.Attribute[2]);
	end	
end

function onDoubleClick(x, y)
	if getValue() == manager_char_stat_handler.StatList.rStatTypesFull.Attribute[2] then
		setValue(manager_char_stat_handler.StatList.rStatTypesFull.Skill[2]);
		window.dt_action_stat1.updateList(manager_char_stat_handler.getSkills());
	elseif getValue() == manager_char_stat_handler.StatList.rStatTypesFull.Skill[2] then
		setValue(manager_char_stat_handler.StatList.rStatTypesFull.Resource[2]);
		window.dt_action_stat1.updateList(manager_char_stat_handler.getAt());
	elseif getValue() == manager_char_stat_handler.StatList.rStatTypesFull.Resource[2] then
		setValue(manager_char_stat_handler.StatList.rStatTypesFull.Attribute[2]);
		window.dt_action_stat1.updateList(FBResourceManager.getResources());
	end	
end


